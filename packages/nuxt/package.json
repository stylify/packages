{
	"name": "@stylify/nuxt",
	"version": "0.5.31",
	"description": "Module for Nuxt v3+ for easier Stylify integration.",
	"keywords": [
		"stylify",
		"nuxtjs",
		"nuxt",
		"ui",
		"css",
		"sass",
		"utility-first",
		"mobile-first",
		"responsive",
		"front-end",
		"framework",
		"web"
	],
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./esm/module.mjs",
			"require": "./lib/module.cjs",
			"types": "./types/*"
		},
		"./types/*": "./types/*",
		"./package.json": "./package.json"
	},
	"main": "lib/module.cjs",
	"module": "esm/module.mjs",
	"types": "types",
	"files": [
		"esm",
		"lib",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "nuxi build playground",
		"dev": "nuxi dev playground",
		"start": "nuxi start playground",
		"pack": "yarn pack"
	},
	"dependencies": {
		"@nuxt/kit": "npm:@nuxt/kit-edge@latest",
		"@stylify/bundler": "^0.5.31",
		"@stylify/stylify": "^0.5.31",
		"@stylify/unplugin": "^0.5.31",
		"normalize-path": "^3.0.0"
	},
	"devDependencies": {
		"@nuxt/content": "^2.0.1",
		"@nuxt/module-builder": "latest",
		"@nuxtjs/eslint-config-typescript": "latest",
		"eslint": "latest",
		"fast-glob": "^3.2.11",
		"nuxt": "npm:nuxt3@latest"
	},
	"gitHead": "a900b73132f964b56e1fc3423648b104d851dd59"
}
