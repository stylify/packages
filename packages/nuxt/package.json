{
	"name": "@stylify/nuxt",
	"version": "0.6.0",
	"description": "Module for Nuxt v3+ for easier Stylify integration.",
	"keywords": [
		"stylify",
		"nuxtjs",
		"nuxt",
		"ui",
		"css",
		"sass",
		"utility-first",
		"mobile-first",
		"responsive",
		"front-end",
		"framework",
		"web"
	],
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./types/module.d.ts",
				"default": "./esm/module.mjs"
			},
			"require": {
				"types": "./types/module.d.ts",
				"default": "./lib/module.cjs"
			}
		},
		"./lib/*": "./lib/*",
		"./esm/*": "./esm/*",
		"./types/*": "./types/*",
		"./package.json": "./package.json"
	},
	"main": "lib/module.cjs",
	"module": "esm/module.mjs",
	"types": "types/module.d.ts",
	"files": [
		"esm",
		"lib",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "nuxi build playground",
		"dev": "nuxi dev playground",
		"start": "nuxi start playground"
	},
	"dependencies": {
		"@nuxt/kit": "^3.5",
		"normalize-path": "^3.0",
		"@stylify/bundler": "workspace:^",
		"@stylify/stylify": "workspace:^",
		"@stylify/unplugin": "workspace:^"
	},
	"devDependencies": {
		"@nuxt/content": "^2.0",
		"@nuxt/schema": "^3.5",
		"@nuxt/module-builder": "latest",
		"@nuxtjs/eslint-config-typescript": "latest",
		"eslint": "latest",
		"fast-glob": "^3.2",
		"nuxt": "^3.5"
	}
}
