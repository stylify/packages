{
	"name": "@stylify/stylify",
	"version": "0.6.0",
	"description": "Stylify generates utility-first CSS dynamically based on what you write. Write HTML. Get CSS.",
	"keywords": [
		"stylify",
		"ui",
		"css",
		"sass",
		"mobile-first",
		"responsive",
		"front-end",
		"framework",
		"web"
	],
	"type": "module",
	"homepage": "https://stylifycss.com",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/stylify"
	},
	"license": "MIT",
	"author": "Vladimír Macháček",
	"exports": {
		".": {
			"import": {
				"default": "./esm/index.mjs",
				"types": "./types/index.d.ts"
			},
			"require": {
				"default": "./lib/index.cjs",
				"types": "./types/index.d.ts"
			}
		},
		"./lib/*": "./lib/*",
		"./esm/*": "./esm/*",
		"./dist/*": "./dist/*",
		"./types/*": "./types/*",
		"./package.json": "./package.json"
	},
	"main": "lib/index.cjs",
	"jsdelivr": "dist/stylify.min.js",
	"unpkg": "dist/stylify.min.js",
	"module": "esm/index.mjs",
	"types": "types/index.d.ts",
	"files": [
		"dist",
		"esm",
		"lib",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	]
}
