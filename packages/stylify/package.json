{
	"name": "@stylify/stylify",
	"version": "0.5.31",
	"description": "Stylify generates utility-first CSS dynamically based on what you write. Write HTML. Get CSS.",
	"keywords": [
		"stylify",
		"ui",
		"css",
		"sass",
		"mobile-first",
		"responsive",
		"front-end",
		"framework",
		"web"
	],
	"homepage": "https://stylifycss.com",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/stylify"
	},
	"license": "MIT",
	"author": "Vladimír Macháček",
	"exports": {
		".": {
			"import": "./esm/index.mjs",
			"require": "./lib/index.cjs",
			"types": "./types/*"
		},
		"./dist/*": "./dist/*",
		"./types/*": "./types/*",
		"./package.json": "./package.json"
	},
	"main": "lib/index.cjs",
	"jsdelivr": "dist/stylify.native.min.js",
	"unpkg": "dist/stylify.native.min.js",
	"module": "esm/index.mjs",
	"types": "types",
	"files": [
		"dist",
		"esm",
		"lib",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	]
}
