{
	"name": "@stylify/cli",
	"description": "Stylify cli.",
	"version": "0.0.1",
	"main": "dist/index.js",
	"module": "esm/index.js",
	"types": "types",
	"scripts": {
		"init": "yarn install",
		"watch": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js -w src",
		"build": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js && yarn types",
		"types": "yarn tsc ./src/index.ts --d --moduleResolution node --emitDeclarationOnly --allowSyntheticDefaultImports --target esnext --outDir types",
		"lint:check": "eslint ./src/*.ts",
		"lint:fix": "yarn lint:check --fix",
		"jest:test": "node --experimental-modules node_modules/.bin/jest --collectCoverage --coverageProvider=v8"
	},
	"bin": {
		"stylify": "dist/bin/stylify"
	},
	"files": [
		"esm",
		"lib",
		"bin",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/cli"
	},
	"keywords": [],
	"author": "Vladimír Macháček",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"homepage": "https://stylify.dev",
	"devDependencies": {
		"@babel/core": "^7.12",
		"@babel/plugin-proposal-class-properties": "^7.14",
		"@babel/plugin-proposal-object-rest-spread": "^7.14",
		"@babel/preset-env": "^7.15",
		"@rollup/plugin-babel": "^5.2",
		"@rollup/plugin-commonjs": "^17.0",
		"@rollup/plugin-node-resolve": "^11.0",
		"rollup-plugin-typescript2": "^0.30",
		"@types/jest": "^26.0",
		"@types/node": "^14.14",
		"@typescript-eslint/eslint-plugin": "^4.11",
		"@typescript-eslint/parser": "^4.11",
		"babel-jest": "^26.6",
		"eslint": "^7.16",
		"jest": "^27.0",
		"jest-environment-jsdom": "^27.0",
		"jest-environment-node": "^27.0",
		"promise": "^8.1",
		"rollup": "^2.35",
		"ts-jest": "^27.0",
		"tslib": "^2.0",
		"typescript": "^4.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@stylify/stylify": "^0.0",
		"@stylify/autoprefixer": "^0.0",
		"globby": "^12.0"
	}
}
